	<xs:complexType name="AbstractOrderShipmentType">
		<xs:annotation>
			<xs:documentation>Represents Shipment Attributes</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ShipmentKey" type="xs:string">
				<xs:annotation>
					<xs:documentation>Unique identifier for a shipment </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentStatus" type="fgl:ConsignmentStatusType">
				<xs:annotation>
					<xs:documentation>Status of the Shippment i.e. PACKED, CANCELLED etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ShipmentStatusDateTime" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>The date/time when the Shipment Status was set or changed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FulfilmentBusinessUnitNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>The BusinesUnit  Number  i..e. FGL Store or DC number used to fulfill the Shipment</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderShipmentPackedType">
		<xs:annotation>
			<xs:documentation>Represents Shipment Attributes</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="fgl:AbstractOrderShipmentType">
				<xs:sequence>
					<xs:element name="ShipmentLines">
						<xs:complexType>
							<xs:sequence>
								<xs:element maxOccurs="unbounded" name="ShipmentLine" type="fgl:ShipmentLineType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OrderShipmentShippedType">
		<xs:annotation>
			<xs:documentation>Represents Shipment Attributes</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="fgl:AbstractOrderShipmentType">
				<xs:sequence>
					<xs:element name="CarrierCode">
						<xs:annotation>
							<xs:documentation>Code that identifies the Carrier Service Used.  restricted to values of CANADA_POST and PUROLATOR</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="CANADA_POST"/>
								<xs:enumeration value="PUROLATOR"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="ShipmentContainers">
						<xs:complexType>
							<xs:sequence>
								<xs:element maxOccurs="unbounded" name="ShipmentContainer" type="fgl:ShipmentContainersType"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
