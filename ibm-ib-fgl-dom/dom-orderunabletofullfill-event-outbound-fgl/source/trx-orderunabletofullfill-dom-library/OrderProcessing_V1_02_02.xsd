		</xs:annotation>
		<xs:sequence>
			<xs:element name="TaxAmounts">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="0" name="TaxAmount" type="fgl:TaxType"/>
