				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="ContainerTrackingNumber" type="xs:string">
				<xs:annotation>
					<xs:documentation>TrackingNo associated to the container</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ContainerEntries">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" name="ContainerEntry" type="fgl:ShipmentLineType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
