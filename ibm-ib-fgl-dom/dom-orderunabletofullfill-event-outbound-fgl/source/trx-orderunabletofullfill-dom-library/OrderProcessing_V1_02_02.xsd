				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1" name="ShippingAddress" nillable="false" type="fgl:CanadaPostAwareShippingAddressType"/>
			<xs:element maxOccurs="1" minOccurs="1" name="RefundEntries" nillable="false">
