			<xs:element maxOccurs="1" minOccurs="0" name="UpcCode" nillable="false" type="xs:string"/>
			<xs:element minOccurs="1" name="BasePrice" nillable="false" type="fgl:PriceType"/>
			<xs:element minOccurs="0" name="TotalProductDiscount" nillable="false" type="xs:double"/>
			<xs:element minOccurs="0" name="TotalOrderDiscount" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Subtotal" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="ReturnReason" nillable="false" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="1" name="ReturnCondition" nillable="false" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="1" name="CategoryClass" nillable="false" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="1" name="CategorySubClass" nillable="false" type="xs:string"/>
