											<xs:simpleContent>
												<xs:extension base="xs:decimal">
													<xs:attributeGroup ref="arts:ElementControlAttributeGroup"/>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="MethodCode" minOccurs="0">
