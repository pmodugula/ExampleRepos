						<xs:sequence>
							<xs:element maxOccurs="1" minOccurs="1" name="Performed" nillable="false" type="xs:dateTime"/>
							<xs:element maxOccurs="1" minOccurs="1" name="PaymentAddress" nillable="false" type="fgl:PaymentAddressType"/>
							<xs:element maxOccurs="1" minOccurs="1" name="Code" nillable="false" type="fgl:CodeType"/>
							<xs:element maxOccurs="1" minOccurs="1" name="OrderNumber" nillable="false" type="xs:string"/>
							<xs:element maxOccurs="1" minOccurs="1" name="CreatedBy" nillable="false" type="xs:string"/>
