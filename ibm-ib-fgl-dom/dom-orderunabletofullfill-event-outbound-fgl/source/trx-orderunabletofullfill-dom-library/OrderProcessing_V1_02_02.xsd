				</xs:complexType>
			</xs:element>
			<xs:element maxOccurs="1" minOccurs="1" name="TotalTaxValue" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Subtotal" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Total" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="TotalDiscount" nillable="false" type="xs:double"/>
			<xs:element maxOccurs="1" minOccurs="1" name="TransactionNumber" nillable="false" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="1" name="OriginalTransactionNumber" nillable="false" type="xs:string"/>
			<xs:element name="Tenders">
				<xs:complexType>
					<xs:sequence>
						<xs:element maxOccurs="unbounded" minOccurs="1" name="Tender" type="fgl:TenderType"/>
