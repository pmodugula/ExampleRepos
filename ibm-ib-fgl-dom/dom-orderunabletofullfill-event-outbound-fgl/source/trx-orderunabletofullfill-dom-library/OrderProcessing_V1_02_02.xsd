			<xs:element maxOccurs="1" minOccurs="1" name="Notes" nillable="true" type="xs:string"/>
			<xs:element maxOccurs="1" minOccurs="1" name="Type" nillable="true" type="fgl:ReimbursementEntryTypeOptionType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ConsignmentEntryCode" nillable="false" type="fgl:CodeType"/>
			<xs:element maxOccurs="1" minOccurs="0" name="ConsignmentCode" nillable="false" type="fgl:CodeType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RefundEntryType">
		<xs:annotation>
			<xs:documentation>Represents RefundEntryType of ReturnRequestRefunded of Order</xs:documentation>
